{
  "name": "toshihiko",
  "version": "1.0.0-alpha.4",
  "description": "Yet another simple ORM for node.js.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "async": "2.1.4",
    "bluebird": "^3.4.6",
    "debug": "^2.3.2",
    "eventemitter2": "^2.1.0",
    "fbbk-json": "^1.0.0",
    "lodash": "4.17.2",
    "moment": "^2.14.1",
    "otrans": "^1.0.0",
    "scarlet-task": "^1.0.0",
    "sqlstring": "^2.0.1",
    "toshihiko-mysqlparser": "^1.0.0"
  },
  "devDependencies": {
    "conventional-changelog-cli": "1.2.0",
    "coveralls": "2.11.15",
    "cz-conventional-changelog": "1.2.0",
    "decache": "^4.1.0",
    "ghooks": "1.3.2",
    "istanbul": "1.0.0-alpha.2",
    "jsxhint": "0.15.1",
    "mocha": "3.1.2",
    "mockjs": "^1.0.1-beta3",
    "mysql": "^2.11.1",
    "mysql2": "^1.1.0",
    "should": "11.1.1",
    "sync-runner": "^0.1.5",
    "toshihiko-memcached": "^1.0.1",
    "validate-commit-msg": "2.8.2"
  },
  "scripts": {
    "test": "make test",
    "lint": "./node_modules/.bin/jsxhint .",
    "validate": "npm ls",
    "commit-msg": "./node_modules/.bin/validate-commit-msg",
    "changelog": "./node_modules/.bin/conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XadillaX/Toshihiko.git"
  },
  "keywords": [
    "orm",
    "mysql",
    "cache"
  ],
  "author": [
    "XadillaX <i@2333.moe>",
    "luicfer <lucifer4he@gmail.com>",
    "lfeng <fengpingan@souche.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/XadillaX/Toshihiko/issues"
  },
  "homepage": "https://github.com/XadillaX/Toshihiko",
  "engines": {
    "node": ">=4.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run lint && npm run validate",
      "commit-msg": "npm run commit-msg",
      "pre-push": "npm test"
    }
  }
}
